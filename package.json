{
  "name": "ng2-playground",
  "version": "0.0.1",
  "description": "Playground for angular 2 related stuff",
  "scripts": {
    "postinstall": "`npm bin`/typings install",
    "build": "`npm bin`/webpack",
    "watch": "`npm bin`/webpack --watch",
    "dev": "`npm bin`/webpack-dev-server",
    "start": "npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MartinNuc/ng2-playground.git"
  },
  "main": "index.js",
  "keywords": [
    "angular",
    "playground"
  ],
  "author": "Martin Nuc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MartinNuc/ng2-playground/issues"
  },
  "homepage": "https://github.com/MartinNuc/ng2-playground#readme",
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "^2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.1",
    "@angular/router": "2.0.0-rc.1",
    "angularfire2": "^2.0.0-beta.0",
    "firebase": "^2.4.2",
    "node-sass": "^3.7.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "es6-shim": "^0.35.1",
    "file-loader": "^0.8.5",
    "raw-loader": "^0.5.1",
    "sass-loader": "^3.2.0",
    "ts-loader": "^0.8.1",
    "typescript": "^1.8.7",
    "typings": "~0.8.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
